  // ts.config
  {
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "declaration": true,
      "emitDeclarationOnly": true,
      "declarationMap": true,

    },
    "include": [
      "src/**/*",
      "types/**/*", "../Frontend/music_app/src/services" ,
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
  

